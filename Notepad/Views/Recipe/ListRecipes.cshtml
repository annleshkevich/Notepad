@using Notepad.ViewModels
@model RecipeListViewModel
 


<h2>All Recipes</h2>
@{
    foreach(var recipe in Model.allRecipes) 
    {
        <div>
        <h2>Name: @recipe.name</h2>
        <h3>Ingredients: @recipe.ingredients</h3>
        <h3>Description: @recipe.description</h3>
        </div>
    } 
}
           